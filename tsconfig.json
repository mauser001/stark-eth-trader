{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target":"ES2022",
      "moduleResolution": "node",
      "sourceMap": false,
      "outDir": "dist",
      "lib": [
        "es2022"
      ],
      "resolveJsonModule": true,
      "isolatedModules": true,
      "allowJs": true,
      "skipLibCheck": true
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }